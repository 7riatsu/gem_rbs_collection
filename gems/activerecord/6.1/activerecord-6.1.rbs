module ActiveRecord
  class Base
    module DelegatedType
      def delegated_type: (Symbol role, types: Array[String], **untyped options) -> void
    end
    extend DelegatedType
  end

  module Enum
    def enum: (Hash[Symbol, (Hash[(Symbol | String), untyped] | Array[Symbol | String] | bool)] definitions) -> void
  end
end
